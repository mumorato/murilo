CREATE VIEW VIEW_SUB_CAT AS 
SELECT
    idSubcategoria,
    nomeCategoria,
    nomeSubcategoria,
    tipoCategoria,
    idCategoria,
    categoriaId
FROM SUBCATEGORIA INNER JOIN CATEGORIA
ON SUBCATEGORIA.categoriaId = CATEGORIA.idCategoria;

